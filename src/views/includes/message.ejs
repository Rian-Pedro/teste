<% if(errors.length > 0) { %>
    <div class="alert alert-danger w-50 container mb-5" role="alert">
        <% errors.forEach(err => { %>
            <%= err %> <br>
        <% }) %>
    </div>
<% } %>

<% if(success.length > 0) { %>
    <div class="alert alert-success w-50 container mb-5" role="success">
        <% success.forEach(suc => { %>
            <%= suc %> <br>
        <% }) %>
    </div>
<% } %>